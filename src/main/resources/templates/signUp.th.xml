<thlogic>
    <attr sel="#header" th:replace="header :: header" />
    <attr sel="#footer" th:replace="footer :: footer" />
    <attr sel="#user-form" th:action th:object="${userForm}" th:method="post">
        <attr sel="#userId" th:field="*{userId}"/>
        <attr sel="#password" th:field="*{password}"/>
        <attr sel="#email" th:field="*{email}"/>
        <attr sel="#nickname" th:field="*{nickname}"/>
        <attr sel="#memo" th:field="*{memo}"/>
    </attr>
</thlogic>