<thlogic>
    <attr sel="#header" th:replace="header :: header" />
    <attr sel="#footer" th:replace="footer :: footer" />
    <attr sel="#form" action="#" method="POST" th:action="@{/signUp}" th:object="${userForm}">
        <attr sel="#userId" type="text" th:field="*{userId}"></attr>
        <attr sel="#password" type="password" th:field="*{password}"></attr>
        <attr sel="#email" type="email" th:field="*{email}"></attr>
        <attr sel="#nickname" type="text" th:field="*{nickname}"></attr>
        <attr sel="#memo" type="text" th:field="*{memo}"></attr>
    </attr>
</thlogic>